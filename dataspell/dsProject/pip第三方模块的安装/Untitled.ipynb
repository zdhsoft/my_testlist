   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "1. 打开报错路径\n",
    "2. 填写以下内容\n",
    "\n",
    "[global]\n",
    "index-url=https://mirrors.aliyun.com/pypi/simple/\n",
    "extra-index-url=\n",
    "  https://pypi.tuna.tsinghua.edu.cn/simple/\n",
    "  https://pypi.mirrors.ustc.edu.cn/simple/\n",
    "  https://pypi.douban.com/simple/\n",
    "  \n",
    "[install]\n",
    "trusted-host=mirrors.aliyun.com"
   ]
  }
