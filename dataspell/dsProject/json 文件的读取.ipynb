    "<code>\n",
    "def my_filePath():\n",
    "    with open('web_downloadFile/filePath.json', encoding='utf-8-sig') as f:\n",
    "        line = f.readline()\n",
    "        d = json.loads(line)\n",
    "        path = d['path']\n",
    "        f.close()\n",
    "        return path\n",
    "</code>"
